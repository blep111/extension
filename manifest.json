{
  "manifest_version": 3,
  "name": "Facebook Cookie Manager",
  "version": "1.0",
  "description": "Import and export Facebook cookies as JSON.",
  "permissions": ["cookies", "tabs", "activeTab"],
  "host_permissions": ["https://facebook.com/*"],
  "action": {
    "default_popup": "popup.html",
    "default_title": "Manage FB Cookies"
  }
}